!============================
! Xft setting
!===============================
Xft.antialias:  true
!Xft.autohint:   true
Xft.hinting:    true
Xft.hintstyle:  hintmedium
!Xft.hintstyle:  hintfull
!Xft.dpi:        96

! Mouse cursor
Xcursor.theme: ComixCursors-LH-Opaque-White-Regular-Slim
!
!===============================
! - Setting for xterm
! --------------------
*customization: -color
XTerm*termName: xterm-256color
XTerm*locale: true
XTerm*utf8Title: true
XTerm*fontMenu*fontdefault*Label: Default
XTerm*faceName: DejaVu Sans Mono:pixelsize=13:antialias=true,xft:WenQuanYi Micro Hei Mono:pixelsize=17
!XTerm*faceNameDoublesize:   DejaVu Sans Mono:pixelsize=11.5:antialias=true:Bold,xft:WenQuanYi Micro Hei Mono:pixelsize=14:Bold
XTerm*faceNameDoublesize: xft:WenQuanYi Micro Hei:antialias=True
XTerm*faceSize:  11
XTerm*faceSize1: 11
XTerm*faceSize2: 11
XTerm*faceSize3: 11
XTerm*faceSize4: 11
XTerm*faceSize5: 11
XTerm*faceSize6: 11
!XTerm*cjkWidth:true
XTerm*scrollBar: false
XTerm*rightScrollBar: false
XTerm*jumpScroll:  false
XTerm*SaveLines: 1000
!XTerm*background:  #0c0d0e
!XTerm*foreground:  #7f8f9f
XTerm*background:  #333333
XTerm*foreground:  #e0e0b5
XTerm*color0:      #101010
XTerm*color1:      #705050
! green + yellow
XTerm*color2:      #60b48a
XTerm*color3:      #f0dfaf
! blue + pURple
XTerm*color4:      #506070
XTerm*color5:      #dc8cc3
! cyan + white
XTerm*color6:      #8cd0d3
XTerm*color7:      #dcdccc
! bright-black + bright-red
XTerm*color8:      #000000
XTerm*color9:      #dca3a3
! bright-green + bright-yellow
!XTerm*color10:     #c3bf9f
XTerm*color10:     #f1cd6e
XTerm*color11:     #f0dfaf
! bright-blue + bright-pURple
XTerm*color12:     #94bff3
XTerm*color13:     #ec93d3
! bright-cyan + bright-white
XTerm*color14:     #93e0e3
XTerm*color15:     #ffffff

! fix some problems with emacs <backspace>
! XTerm.ttyModes:         erase ^?
! XTerm.VT100.Translations: \
!         #override <Key>BackSpace: string(0x7f)\n\
! 		  <Key>Delete:    string(0x1b) string("[3~")
! fix <Alt> key
XTerm.VT100.EightBitInput: false
XTerm.VT100.EightBitControl : false
XTerm.VT100.EightBitOutput : true
XTerm.metaSendsEscape: true
! Disable Alt+Enter=maximize
XTerm.fullscreen: never
XTerm.*.backarrowKey: false


