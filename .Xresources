!===============================
! URxvT
!===============================
URxvt.preeditType:Root

!URxvt.background:  #0c0d0e
!URxvt.foreground:  #7f8f9f
URxvt.background:  #333333
URxvt.foreground:  #e0e0b5
URxvt.color0:      #101010
URxvt.color1:      #705050
! green + yellow
URxvt.color2:      #60b48a
URxvt.color3:      #f0dfaf
! blue + pURple
URxvt.color4:      #506070
URxvt.color5:      #dc8cc3
! cyan + white
URxvt.color6:      #8cd0d3
URxvt.color7:      #dcdccc
! bright-black + bright-red
URxvt.color8:      #000000
URxvt.color9:      #dca3a3
! bright-green + bright-yellow
!URxvt.color10:     #c3bf9f
URxvt.color10:     #f1cd6e
URxvt.color11:     #f0dfaf
! bright-blue + bright-pURple
URxvt.color12:     #94bff3
URxvt.color13:     #ec93d3
! bright-cyan + bright-white
URxvt.color14:     #93e0e3
URxvt.color15:     #ffffff

! scroll set
URxvt.scrollBar:false
URxvt.scrollBar_right:True
URxvt.scrollBar_floating:True
URxvt.scrollstyle:plain
URxvt.scrollWithBuffer:false
URxvt.saveLines:5000

! font set
URxvt.font: xft:DejaVu Sans Mono:pixelsize=13:antialias=true,xft:WenQuanYi Micro Hei:size=16:antialias=true
!URxvt.font: xft:DejaVu Sans Mono:pixelsize=13:antialias=true,xft:WenQuanYi Micro Hei Mono:pixelsize=25:antialias=true
!URxvt.boldFont:xft:DejaVu Sans Mono:pixelsize=11.5:antialias=true:Bold,xft:WenQuanYi Micro Hei Mono:pixelsize=14:Bold
!URxvt.faceNameDoublesize: WenQuanYi Micro Hei:antialias=True

! window geometry set
!URxvt.geometry: 80x30
!URxvt.title:Prongs-Rxvt

!! Tab set
!URxvt.perl-ext-common: default,tabbed,matcher
!URxvt.tabbed.tab-fg: 12
!URxvt.tabbed.tab-bg: 0
!URxvt.tabbed.tabbar-fg: 4

Rxvt.menu:/etc/X11/rxvt.menu
!Rxvt.preeditType:Root
!Rxvt.transparent:true
!Rxvt.shading:50

URxvt.urlLauncher: firefox
URxvt.matcher.button: 1
!URxvt.perl-ext-common: matcher

-----------------------------------------------------------------------------
! rofi
rofi.font: DejaVu Sans Mono 12
rofi.show-icons: true
